{
    "demonstrable.autoUpdateLanguageServer": false,
    "python.venvPath": "/Users/abingham/.virtualenvs",
    "explorer.confirmDragAndDrop": false,
    "editor.minimap.enabled": false,
    "editor.multiCursorModifier": "alt",
    "git.confirmSync": false,
    "python.venvFolders": [
        ".virtualenvs",
    ],
    "python.linting.flake8Args": [
        "--max-line-length=120"
    ],
    "terminal.integrated.rendererType": "dom",
    "terminal.integrated.tabs.enabled": false,
    "python.linting.flake8Enabled": true,
    "files.associations": {
        "*.gjenta": "python",
        "SConstruct": "python",
        "SConscript": "python"
    },
    "[elm]": {
        "editor.formatOnSave": true
    },
    "explorer.confirmDelete": false,
    "center-editor-window.threeStateToggle": true,
    "diffEditor.renderSideBySide": false,
    "workbench.activityBar.visible": true,
    "editor.accessibilitySupport": "off",
    "workbench.statusBar.visible": true,
    "workbench.list.openMode": "doubleClick",
    "explorer.autoReveal": false,
    "restructuredtext.linter.run": "onSave",
    "python.languageServer": "Pylance",
    "workbench.editorAssociations": {
        "*.ipynb": "jupyter-notebook",
        "*.mov": "default"
    },
    "terminal.integrated.commandsToSkipShell": [
        "language-julia.interrupt"
    ],
    "around.pairs": {
        "(": [
            "(",
            ")"
        ],
        "{": [
            "{",
            "}"
        ],
        "<": [
            "<",
            ">"
        ],
        "[": [
            "[",
            "]"
        ]
    },
    "workbench.editor.showTabs": true,
    "magit.display-buffer-function": "same-column",
    "diffEditor.ignoreTrimWhitespace": false,
    "jupyter.askForKernelRestart": false,
    "[plaintext]": {
        "editor.wordBasedSuggestions": false,
        "editor.quickSuggestions": {
            "comments": "off",
            "strings": "off",
            "other": "off"
        }
    },
    "[restructuredtext]": {
        "editor.wordBasedSuggestions": false,
        "editor.quickSuggestions": {
            "comments": "off",
            "strings": "off",
            "other": "off"
        }
    },
    "[markdown]": {
        "editor.wordBasedSuggestions": false,
        "editor.quickSuggestions": {
            "comments": "off",
            "strings": "off",
            "other": "off"
        }
    },
    "jupyter.alwaysTrustNotebooks": true,
    "gitlens.views.branches.branches.layout": "list",
    "redhat.telemetry.enabled": true,
    "files.autoSave": "afterDelay",
    "editor.tabSize": 8,
    "editor.cursorStyle": "line",
    "editor.insertSpaces": false,
    "editor.lineNumbers": "on",
    "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
    "editor.wordWrap": "off",
    "notebook.cellToolbarLocation": {
        "default": "right",
        "jupyter-notebook": "left"
    },
    "[python]": {
        "editor.insertSpaces": true,
        "editor.tabSize": 4,
        "editor.detectIndentation": true,
        "editor.wordBasedSuggestions": false
    },
    "workbench.editor.untitled.hint": "hidden",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "[rust]": {
        "editor.defaultFormatter": "rust-lang.rust"
    },
    "[html]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "esbonio.server.enabled": false,
    "[typescript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "workbench.colorTheme": "Default Light+",
    "python.terminal.activateEnvInCurrentTerminal": true,
    "[javascript]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[jsonc]": {
        "editor.defaultFormatter": "esbenp.prettier-vscode"
    }
}
