    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "project-scopes.activeScope": "bullet_list",
  "project-scopes.enabled": false,
  "terminal.integrated.enableMultiLinePasteWarning": false,
  "cmake.showOptionsMovedNotification": false,
  "project-scopes.scopes": {
    "base": {
      "included": [],
      "excluded": []
    },
  },
  "[markdown][plaintext][python][restructuredtext]": {
    "editor.wordBasedSuggestions": "off"
  },
  "window.zoomLevel": 1,
  "python.analysis.autoImportCompletions": true,
