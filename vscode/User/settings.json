{
  "cmake.ignoreCMakeListsMissing": true,
  "python.venvPath": "/Users/abingham/.virtualenvs",
  "explorer.confirmDragAndDrop": false,
  "editor.minimap.enabled": false,
  "editor.multiCursorModifier": "alt",
  "git.confirmSync": false,
  "python.venvFolders": [".virtualenvs"],
  "files.associations": {
    "*.gjenta": "python",
    "SConstruct": "python",
    "SConscript": "python"
  },
  "[elm]": {
    "editor.formatOnSave": true
  },
  "explorer.confirmDelete": false,
  "center-editor-window.threeStateToggle": true,
  "diffEditor.renderSideBySide": false,
  "editor.accessibilitySupport": "off",
  "workbench.statusBar.visible": true,
  "workbench.list.openMode": "doubleClick",
  "explorer.autoReveal": false,
  "restructuredtext.linter.run": "onSave",
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter-notebook",
    "*.mov": "default"
  },
  "terminal.integrated.commandsToSkipShell": ["language-julia.interrupt"],
  "around.pairs": {
    "(": ["(", ")"],
    "{": ["{", "}"],
    "<": ["<", ">"],
    "[": ["[", "]"]
  },
  "workbench.editor.showTabs": "multiple",
  "magit.display-buffer-function": "same-column",
  "diffEditor.ignoreTrimWhitespace": false,
  "jupyter.askForKernelRestart": false,
  "[plaintext]": {
    "editor.wordBasedSuggestions": "off",
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    }
  },
  "[restructuredtext]": {
    "editor.wordBasedSuggestions": "off",
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    }
  },
  "[markdown]": {
    "editor.wordBasedSuggestions": "off",
    "editor.quickSuggestions": {
      "comments": "off",
      "strings": "off",
      "other": "off"
    }
  },
  "redhat.telemetry.enabled": true,
  "files.autoSave": "afterDelay",
  "editor.cursorStyle": "line",
  "editor.lineNumbers": "on",
  "editor.wordSeparators": "/\\()\"':,.;<>~!@#$%^&*|+=[]{}`?-",
  "editor.wordWrap": "off",
  "notebook.cellToolbarLocation": {
    "default": "right",
    "jupyter-notebook": "left"
  },
  "[python]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 4,
    "editor.detectIndentation": true,
    "editor.wordBasedSuggestions": "off",
    "editor.formatOnType": true,
    "editor.defaultFormatter": "charliermarsh.ruff"
  },
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/Thumbs.db": true,
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "[rust]": {
    "editor.defaultFormatter": "rust-lang.rust"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "esbonio.server.enabled": false,
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.colorTheme": "Visual Studio Light",
  "python.terminal.activateEnvInCurrentTerminal": true,
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "editor.inlineSuggest.enabled": true,
  "terminal.integrated.env.osx": {
    "FIG_NEW_SESSION": "1"
  },
  "[xml]": {
    "editor.defaultFormatter": ""
  },
  "terminal.integrated.tabs.location": "left",
  "terminal.integrated.tabs.enabled": false,
  "github.copilot.enable": {
    "*": true,
    "plaintext": false,
    "markdown": false,
    "scminput": false,
    "yaml": false
